{
  "manifest_version": 3,
  "name": "Outlook Helper",
  "version": "3.0",
  "description": "try to take over the world!",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://outlook.office.com/mail/*",
        "https://outlook.office365.com/mail/*"
      ],
      "js": [
        "libs/gsap.min.js",
        "libs/Draggable.min.js",
        "libs/dom-to-image-more-fonts.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "clipboardWrite",
    "scripting"
  ],
  "host_permissions": [
    "https://outlook.office.com/*",
    "https://outlook.office365.com/*"
  ],
  "web_accessible_resources": [{
    "resources": [
        "libs/*"
    ],
    "matches": [
        "<all_urls>"
    ]
  }]
}